//MySql Assignment Code 


show databases;
create database Ecommerce;
use Ecommerce;

CREATE TABLE product (
    product_id INT PRIMARY KEY,
    product_name VARCHAR(255),
    product_description TEXT,
    product_refundable VARCHAR(40),
    owner VARCHAR(100)
);

CREATE TABLE orders (
	order_id VARCHAR(45),
    product_id 	VARCHAR(45),
    user_id VARCHAR(45),
    status VARCHAR(50),
    oderscol VARCHAR(45),
    products_product_id VARCHAR(255),
    users_user_id VARCHAR(20)
);

CREATE TABLE users(
user_id VARCHAR(20),
first_name VARCHAR(50),
last_name VARCHAR(50),
pincode VARCHAR(10),
address_pincode VARCHAR(10)
);
create table address(
 pincode VARCHAR(10),
 city VARCHAR(255),
 state VARCHAR(255)
);
SHOW TABLES;
